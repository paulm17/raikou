{
  "name": "postcss-purgecss-raikou",
  "version": "0.0.1-beta.8",
  "description": "PostCSS plugin for purging used css for Raikou applications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "peerDependencies": {
    "postcss": ">=8.0.0"
  },
  "dependencies": {
    "postcss-nested": "^6.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/postcss-mixins": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "@unocss/config": "^0.57.4",
    "@unocss/core": "^0.57.4",
    "@unocss/rule-utils": "^0.57.4",
    "css-tree": "^2.3.1",
    "eslint": "^8.43.0",
    "fast-glob": "^3.3.2",
    "jest": "^29.5.0",
    "magic-string": "^0.30.5",
    "postcss": "^8.4.32",
    "prettier": "^2.8.8",
    "tslib": "^2.6.2",
    "ts-jest": "^29.1.0",
    "tsup": "^8.0.1",
    "typescript": "^5.1.3"
  },
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs --clean",
    "lint": "eslint src --cache",
    "format": "prettier src",
    "format:check": "prettier --check src",
    "typecheck": "tsc --noEmit",
    "jest": "jest",
    "prepublish": "yarn test && yarn build",
    "test": "npm run lint && npm run format:check && npm run typecheck && npm run jest"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:paulm17/postcss-reset-raikou.git"
  },
  "keywords": [
    "postcss",
    "raikou",
    "react",
    "css"
  ],
  "author": "notme@gmail.com",
  "license": "MIT"
}
