{
  "name": "@raikou/client",
  "version": "0.0.1-alpha.6",
  "main": "index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist/**"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs --clean",
    "lint": "eslint \"**/*.ts*\"",
    "clean": "rimraf dist .turbo",
    "typecheck": "tsc --noEmit",
    "build:fast": "tsup src",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "clean-package": "../../../clean-package.config.json",
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@raikou/accordion": "workspace:*",
    "@raikou/alert": "workspace:*",
    "@raikou/appshell": "workspace:*",
    "@raikou/autocomplete": "workspace:*",
    "@raikou/avatar": "workspace:*",
    "@raikou/checkbox": "workspace:*",
    "@raikou/chip": "workspace:*",
    "@raikou/collapse": "workspace:*",
    "@raikou/color-picker": "workspace:*",
    "@raikou/combobox": "workspace:*",
    "@raikou/drawer": "workspace:*",
    "@raikou/hover-card": "workspace:*",
    "@raikou/inline-input": "workspace:*",
    "@raikou/input": "workspace:*",
    "@raikou/input-base": "workspace:*",
    "@raikou/modal": "workspace:*",
    "@raikou/modal-base": "workspace:*",
    "@raikou/multi-select": "workspace:*",
    "@raikou/native-select": "workspace:*",
    "@raikou/pagination": "workspace:*",
    "@raikou/pill": "workspace:*",
    "@raikou/pills-input": "workspace:*",
    "@raikou/popover": "workspace:*",
    "@raikou/portal": "workspace:*",
    "@raikou/progress": "workspace:*",
    "@raikou/rating": "workspace:*",
    "@raikou/scroll-area": "workspace:*",
    "@raikou/slider": "workspace:*",
    "@raikou/switch": "workspace:*",
    "@raikou/tabs": "workspace:*",
    "@raikou/table": "workspace:*",
    "@raikou/tags-input": "workspace:*",
    "@raikou/text-input": "workspace:*",
    "@raikou/tooltip": "workspace:*",
    "type-fest": "^3.6.1",
    "tslib": "^2.5.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/node": "^20.3.3",
    "clean-package": "2.2.0",
    "tsup": "^7.1.0",
    "typescript": "^5.1.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
