{
  "name": "@raikou/badge",
  "version": "0.0.1-beta.8",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist/**"
  ],
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "build": "tsup src/index.ts --config ../../../../configuration/tsup/tsup.config.ts && pnpm minify",
    "lint": "eslint \"**/*.ts*\"",
    "clean": "rimraf dist .turbo",
    "minify": "uglifyjs --compress --mangle --output dist/index.js -- dist/index.js && uglifyjs --compress --mangle --output dist/index.mjs -- dist/index.mjs",
    "typecheck": "tsc --noEmit",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "clean-package": "../../../../clean-package.config.json",
  "peerDependencies": {
    "@raikou/core": ">=0.0.1-alpha.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@raikou/core": "workspace:*",
    "tslib": "^2.6.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.23",
    "@types/react-dom": "^18.2.8",
    "@types/node": "^20.3.3",
    "clean-package": "2.2.0",
    "typescript": "^5.1.6"
  },
  "publishConfig": {
    "access": "public"
  },
  "tsup": {
    "format": [
      "esm"
    ],
    "loader": {
      ".css": "local-css"
    },
    "dts": true,
    "sourcemap": true,
    "clean": true
  }
}
