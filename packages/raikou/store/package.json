{
  "name": "@raikou/store",
  "version": "0.0.1-alpha.17",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./cjs/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "license": "MIT",
  "author": "Vitaly Rtishchev <rtivital@gmail.com>",
  "sideEffects": false,
  "homepage": "https://raikou.dev",
  "repository": {
    "url": "https://github.com/raikoudev/raikou.git",
    "type": "git",
    "directory": "src/raikou-store"
  },
  "keywords": [
    "react",
    "next",
    "nextjs",
    "library",
    "frontend",
    "react-hooks",
    "hooks",
    "state"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/plugin.ts --dts --format esm,cjs --clean",
    "lint": "eslint \"**/*.ts*\"",
    "clean": "rimraf dist .turbo",
    "typecheck": "tsc --noEmit",
    "build:fast": "tsup src",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "clean-package": "../../../clean-package.config.json",
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "clean-package": "2.2.0",
    "tsup": "^7.1.0"
  }
}
