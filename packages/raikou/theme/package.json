{
  "name": "@raikou/theme",
  "private": true,
  "version": "0.0.1-beta.8",
  "main": "index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./plugin.js": {
      "types": "./dist/plugin.d.ts",
      "import": "./dist/plugin.mjs",
      "require": "./dist/plugin.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist/**"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs --clean && pnpm minify",
    "minify": "jsmin -o dist/index.js dist/index.js && jsmin -o dist/index.mjs dist/index.mjs",
    "lint": "eslint \"**/*.ts*\""
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "dot-path-value": "^0.0.10",
    "type-fest": "^4.0.0",
    "tslib": "^2.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.3.3",
    "@types/react": "^18.2.23",
    "@types/react-dom": "^18.2.8",
    "typescript": "^5.1.6"
  },
  "publishConfig": {
    "access": "public"
  }
}
