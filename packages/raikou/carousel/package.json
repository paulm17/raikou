{
  "name": "@raikou/carousel",
  "description": "Embla based carousel",
  "version": "0.0.1-beta.8",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --config ../../../configuration/tsup/tsup.config.ts",
    "lint": "eslint \"**/*.ts*\"",
    "clean": "rimraf dist .turbo",
    "typecheck": "tsc --noEmit",
    "build:fast": "tsup src",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "clean-package": "../../../clean-package.config.json",
  "peerDependencies": {
    "@raikou/core": ">=0.0.1-alpha.1",
    "@raikou/hooks": ">=0.0.1-alpha.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@raikou/core": "workspace:*",
    "@raikou/hooks": "workspace:*",
    "embla-carousel-autoplay": "^8.0.0-rc11",
    "embla-carousel-react": "^7.0.0",
    "tslib": "^2.6.1"
  },
  "devDependencies": {
    "clean-package": "2.2.0"
  }
}
