    "@raikou/theme-icon": "workspace:*",
    "@raikou/title": "workspace:*",
    "@raikou/unstyled-button": "workspace:*",
    "@raikou/visually-hidden": "workspace:*",
    "type-fest": "^4.0.0",
    "tslib": "^2.6.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/node": "^20.3.3",
    "clean-package": "2.2.0",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6"
  },
  "publishConfig": {
    "access": "public"
  }
}
