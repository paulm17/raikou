{
  "name": "@raikou/core",
  "version": "0.0.1-alpha.2",
  "main": "index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./plugin.js": {
      "types": "./dist/plugin.d.ts",
      "import": "./dist/plugin.mjs",
      "require": "./dist/plugin.js"
    },
    "./styles.css": "./dist/index.css"
  },
  "files": [
    "dist/**"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts src/plugin.ts --dts --format esm,cjs --clean",
    "lint": "eslint \"**/*.ts*\""
  },
  "peerDependencies": {
    "@raikou/hooks": ">=0.0.1-alpha.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.2"
  },
  "dependencies": {
    "@raikou/hooks": "workspace:*",
    "clsx": "^1.1.1",
    "type-fest": "^3.6.1",
    "tslib": "^2.5.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/node": "^20.3.3",
    "tsconfig": "workspace:*",
    "tsup": "^7.1.0",
    "typescript": "^5.1.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
