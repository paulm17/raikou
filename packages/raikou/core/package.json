{
  "name": "@raikou/core",
  "version": "0.0.1-beta.8",
  "main": "index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./plugin.js": {
      "types": "./dist/plugin.d.ts",
      "import": "./dist/plugin.mjs",
      "require": "./dist/plugin.js"
    },
    "./styles.css": "./dist/index.css"
  },
  "files": [
    "dist/**"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts src/plugin.ts --dts --format esm,cjs --clean && pnpm minify",
    "lint": "eslint \"**/*.ts*\"",
    "clean": "rimraf dist .turbo",
    "minify": "jsmin -o dist/index.js dist/index.js && jsmin -o dist/index.mjs dist/index.mjs",
    "typecheck": "tsc --noEmit",
    "build:fast": "tsup src",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "clean-package": "../../../clean-package.config.json",
  "peerDependencies": {
    "@raikou/hooks": ">=0.0.1-alpha.1",
    "@raikou/global-store": ">=0.0.1-alpha.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@raikou/hooks": "workspace:*",
    "@raikou/global-store": "workspace:*",
    "clsx": "2.0.0",
    "pure-store": "^1.2.0",
    "type-fest": "^4.0.0",
    "tslib": "^2.6.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.23",
    "@types/react-dom": "^18.2.8",
    "@types/node": "^20.3.3",
    "@unocss/preset-attributify": "^0.58.0",
    "clean-package": "2.2.0",
    "typescript": "^5.1.6"
  },
  "publishConfig": {
    "access": "public"
  }
}
