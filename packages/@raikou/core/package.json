    "react-textarea-autosize": "^8.5.4",
    "type-fest": "^4.26.1"
  },
  "devDependencies": {
    "@raikou-tests/core": "workspace:*",
    "@raikou/emotion": "workspace:*",
    "@raikou/hooks": "workspace:*",
