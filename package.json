{
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "build:postcss": "turbo run build --filter=postcss-preset-raikou --filter=postcss-purgecss-raikou --filter=postcss-unocss-raikou",
    "build:github": "./buildAll.sh",
    "build:fast": "turbo run build:fast --filter=!./examples/* --filter=!*props-docs",
    "build:prop-docs": "pnpm typedocs && pnpm --filter=*props-docs build",
    "build:support_files": "esno packages/raikou/system/scripts/generate-support-files",
    "typedocs": "pnpm --filter=./packages/components/** typedocs",
    "dev": "turbo run dev --filter=!./examples/*",
    "typecheck": "tsc --noEmit",
    "build:theme": "cd packages/components/theme && pnpm build:fast",
    "test": "jest",
    "lint": "eslint packages/**/**/src --ext .ts,.tsx --config .eslintrc --cache",
    "format:check": "prettier --check packages/**/**/src --cache",
    "format:write": "prettier --write packages/**/**/src --cache",
    "build:addon": "cd tooling/storybook-addon && pnpm build",
    "clean:storybook": "rm -rf node_modules/.cache/storybook",
    "build:storybook": "pnpm build:addon && storybook build",
    "serve:storybook": "pnpx serve storybook-static",
    "clean": "pnpm -r --parallel exec rimraf dist .turbo *.log",
    "changelog": "tsx scripts/changelog.ts",
    "changelog:latest": "tsx scripts/changelog.ts --latest",
    "changelog:commit": "git add . && git commit -am 'docs: add changelog' && git push",
    "changelog:format": "prettier --write .changelog",
    "slack": "tsx scripts/slack.ts",
    "csb": "tsx scripts/csb.ts",
    "create:pkg": "plop component",
    "version": "changeset version",
    "release": "changeset publish",
    "version:dev": "changeset version --snapshot dev",
    "release:dev": "changeset publish --tag dev",
    "sandbox": "tsx scripts/sandbox.ts",
    "notify:release": "npm-run-all changelog:write slack changelog:commit",
    "turbo_build": "turbo run build",
    "turbo_dev": "turbo run dev",
    "turbo_lint": "turbo run lint",
    "turbo_format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "_build": "turbo run build",
    "_dev": "turbo run dev",
    "storybook": "pnpm --filter docs run storybook",
    "_lint": "turbo run lint",
    "jest": "jest",
    "_prettier:test": "prettier --check \"**/*.{ts,tsx}\"",
    "_prettier:write": "prettier --write \"**/*.{ts,tsx}\""
  },
  "dependencies": {
    "@tabler/icons-react": "^2.30.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "0.4.8",
    "@changesets/cli": "2.26.2",
    "@changesets/get-release-plan": "3.0.17",
    "@changesets/types": "5.2.1",
    "@commitlint/cli": "14.1.0",
    "@commitlint/config-conventional": "14.1.0",
    "@octokit/rest": "^19.0.4",
    "@sucrase/jest-plugin": "^2.1.1",
    "@testing-library/dom": "8.13.0",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.2.0",
    "@testing-library/user-event": "14.2.0",
    "@types/edit-json-file": "^1.6.1",
    "@types/fs-extra": "11.0.1",
    "@types/jest": "^29.5.1",
    "@types/mkdirp": "1.0.2",
    "@types/node": "^18.0.0",
    "@types/react": "^18.2.23",
    "@types/react-dom": "^18.2.8",
    "@types/testing-library__jest-dom": "5.14.8",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "@turbo/gen": "^1.9.7",
    "@types/jest-axe": "^3.5.1",
    "cssnano": "^6.0.1",
    "esbuild-css-modules-plugin": "^3.1.0",
    "esbuild-plugin-postcss2": "^0.1.2",
    "eslint": "8.45.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.11.0",
    "esno": "^4.0.0",
    "fast-glob": "^3.3.2",
    "find-packages": "9.0.13",
    "find-up": "^6.3.0",
    "fs-extra": "11.1.1",
    "hash-css-selector": "^1.0.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.1",
    "jest-axe": "^8.0.0",
    "jest-environment-jsdom": "^29.6.1",
    "jest-watch-typeahead": "^2.2.2",
    "jsmin": "^1.0.1",
    "lint-staged": "11.2.6",
    "postcss": "^8.4.31",
    "postcss-discard-duplicates": "^6.0.0",
    "postcss-import": "^15.1.0",
    "postcss-modules": "^6.0.0",
    "postcss-preset-raikou": "workspace:*",
    "prettier": "^3.0.0",
    "rimraf": "3.0.2",
    "tslib": "^2.6.2",
    "tsup": "^8.0.1",
    "tsx": "^3.8.1",
    "turbo": "latest",
    "type-fest": "^4.8.3",
    "typescript": "^5.1.6"
  },
  "pnpm": {
    "overrides": {
      "react": "^18.2.0",
      "react-dom": "^18.2.0"
    }
  },
  "engines": {
    "node": ">=16.x"
  },
  "packageManager": "pnpm@7.15.0",
  "name": "raikou"
}
